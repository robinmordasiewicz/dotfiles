{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "alignment": "left",
      "type": "prompt",
      "segments": [
        {
          "foreground": "#d3d7cf",
          "background": "transparent",
          "style": "powerline",
          "template": "‚ï≠‚îÄ",
          "type": "text"
        },
        {
          "background": "#0D71CE",
          "foreground": "#d3d7cf",
          "leading_diamond": "<#0D71CE,transparent>ÓÇ≤",
          "properties": {
            "home_icon": "üè†",
            "style": "full"
          },
          "style": "diamond",
          "template": " {{ .Path }} ",
          "type": "path"
        },
        {
          "background": "#17B305",
          "foreground": "#d3d7cf",
          "background_templates": [
            "{{ if or (.Working.Changed) (.Staging.Changed) }}#c4a000{{ end }}",
            "{{ if and (gt .Ahead 0) (gt .Behind 0) }}#f26d50{{ end }}",
            "{{ if gt .Ahead 0 }}#89d1dc{{ end }}",
            "{{ if gt .Behind 0 }}#4e9a06{{ end }}"
          ],
          "powerline_symbol": "ÓÇ∞<#d3d7cf,transparent>",
          "properties": {
            "fetch_stash_count": true,
            "fetch_status": true
          },
          "style": "powerline",
          "template": "<foreground,background>{{ .UpstreamIcon }}{{ .HEAD }}{{if .BranchStatus }} {{ .BranchStatus }}{{ end }}{{ if .Working.Changed }} ÔÅÑ {{ .Working.String }}{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Staging.Changed }} ÔÅÜ {{ .Staging.String }}{{ end }}{{ if gt .StashCount 0 }} Ó≠ã {{ .StashCount }}{{ end }} ",
          "type": "git"
        }
      ]
    },
    {
      "alignment": "right",
      "filler": "‚îÄ",
      "type": "prompt",
      "segments": [
        {
          "background": "transparent",
          "background_templates": [
            "{{ if gt .Code 0 }}#cc2222{{ end }}"
          ],
          "invert_powerline": true,
          "properties": {
            "always_enabled": true
          },
          "style": "powerline",
          "template": " {{ if gt .Code 0 }}{{ reason .Code }}{{ else }}‚úÖ{{ end }} ",
          "type": "status"
        },
        {
          "background": "#689f63",
          "foreground": "#ffffff",
          "invert_powerline": true,
          "powerline_symbol": "ÓÇ≤",
          "properties": {
            "fetch_version": true
          },
          "style": "powerline",
          "template": " {{ if .PackageManagerIcon }}{{ .PackageManagerIcon }} {{ end }}{{ .Full }} Óúò ",
          "type": "node"
        },
        {
          "background": "#00acd7",
          "foreground": "#111111",
          "invert_powerline": true,
          "powerline_symbol": "ÓÇ≤",
          "properties": {
            "fetch_version": true
          },
          "style": "powerline",
          "template": " {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} Óòß ",
          "type": "go"
        },
        {
          "background": "#4063D8",
          "foreground": "#111111",
          "invert_powerline": true,
          "powerline_symbol": "ÓÇ≤",
          "properties": {
            "fetch_version": true
          },
          "style": "powerline",
          "template": " {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} Óò§ ",
          "type": "julia"
        },
        {
          "background": "#AE1401",
          "foreground": "#ffffff",
          "invert_powerline": true,
          "powerline_symbol": "ÓÇ≤",
          "properties": {
            "display_mode": "files",
            "fetch_version": true
          },
          "style": "powerline",
          "template": " {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} Óûë ",
          "type": "ruby"
        },
        {
          "background": "#FEAC19",
          "foreground": "#ffffff",
          "invert_powerline": true,
          "powerline_symbol": "ÓÇ≤",
          "properties": {
            "display_mode": "files",
            "fetch_version": false
          },
          "style": "powerline",
          "template": " {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} ÔÉß",
          "type": "azfunc"
        },
        {
          "background_templates": [
            "{{if contains \"default\" .Profile}}#FFA400{{end}}",
            "{{if contains \"jan\" .Profile}}#f1184c{{end}}"
          ],
          "foreground": "#ffffff",
          "invert_powerline": true,
          "powerline_symbol": "ÓÇ≤",
          "properties": {
            "display_default": false
          },
          "style": "powerline",
          "template": " {{ .Profile }}{{ if .Region }}@{{ .Region }}{{ end }} Óû≠ ",
          "type": "aws"
        },
        {
          "background": "#622188",
          "foreground": "#d3d7cf",
          "invert_powerline": true,
          "powerline_symbol": "ÓÇ≤",
          "trailing_diamond": "<#0D71CE,#0D71CE>ÓÇ∞<transparent,transparent>",
          "properties": {
            "display_mode": "environment",
            "fetch_virtual_env": true,
            "home_enabled": true
          },
          "style": "powerline",
          "template": " {{ if .Error }}{{ .Error }}{{ else }}{{ .Venv }}{{ end }} <#FFDC4B,#622188>Óàµ ",
          "type": "python"
        },
        {
          "type": "az",
          "style": "powerline",
          "foreground": "#d3d7cf",
          "background": "#0D71CE",
          "powerline_symbol": "<background,parentBackground>ÓÇ≤<foreground,background>",
          "template": "<foreground,background> ÓØò {{ .Name }}<foreground,background>"
        },
        {
          "type": "session",
          "style": "diamond",
          "foreground": "#d3d7cf",
          "background": "#0D71CE",
          "trailing_diamond": "<#0D71CE,transparent>ÓÇ∞<#d3d7cf,transparent>",
          "template": "<foreground,background>{{ if .SSHSession }}‚ö°{{ end }}{{ .HostName }}<foreground,background>"
        },
        {
          "background": "transparent",
          "foreground": "#d3d7cf",
          "style": "powerline",
          "template": "<#d3d7cf,transparent>‚îÄ‚ïÆ<#d3d7cf,transparent>",
          "type": "text"
        }
      ]
    },
    {
      "alignment": "left",
      "newline": true,
      "type": "prompt",
      "segments": [
        {
          "background": "transparent",
          "foreground": "#d3d7cf",
          "style": "powerline",
          "template": "<#d3d7cf,transparent>‚ï∞‚îÄ<#d3d7cf,transparent>",
          "type": "text"
        }
      ]
    },
    {
      "type": "rprompt",
      "segments": [
        {
          "foreground": "#d3d7cf",
          "background": "transparent",
          "style": "powerline",
          "template": "<#d3d7cf,transparent>‚îÄ‚ïØ<#d3d7cf,transparent>",
          "type": "text"
        }
      ]
    }
  ],
  "console_title_template": "{{ .Shell }} in {{ .Folder }}",
  "secondary_prompt": {
    "background": "transparent",
    "foreground": "#ffffff",
    "template": "-> "
  },
  "transient_prompt": {
    "background": "transparent",
    "foreground": "#FC0108",
    "template": "> "
  },
  "final_space": true,
  "version": 2
}
