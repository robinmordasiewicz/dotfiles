{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "alignment": "left",
      "type": "prompt",
      "segments": [
        {
          "foreground": "#d3d7cf",
          "background": "transparent",
          "style": "powerline",
          "template": "╭─",
          "type": "text"
        },
        {
          "background": "#0D71CE",
          "foreground": "#e4e4e4",
          "leading_diamond": "<#0D71CE,transparent>",
          "properties": {
            "home_icon": "~",
            "style": "full"
          },
          "style": "diamond",
          "template": " {{ .Path }} ",
          "type": "path"
        },
        {
          "background": "#17B305",
          "background_templates": [
            "{{ if or (.Working.Changed) (.Staging.Changed) }}#c4a000{{ end }}",
            "{{ if and (gt .Ahead 0) (gt .Behind 0) }}#f26d50{{ end }}",
            "{{ if gt .Ahead 0 }}#89d1dc{{ end }}",
            "{{ if gt .Behind 0 }}#4e9a06{{ end }}"
          ],
          "foreground": "#000000",
          "powerline_symbol": "",
          "properties": {
            "fetch_stash_count": true,
            "fetch_status": true
          },
          "style": "powerline",
          "template": " {{ .UpstreamIcon }}{{ .HEAD }}{{if .BranchStatus }} {{ .BranchStatus }}{{ end }}{{ if .Working.Changed }}  {{ .Working.String }}{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Staging.Changed }}  {{ .Staging.String }}{{ end }}{{ if gt .StashCount 0 }}  {{ .StashCount }}{{ end }} ",
          "type": "git"
        }
      ]
    },
    {
      "alignment": "right",
      "filler": "─",
      "type": "prompt",
      "segments": [
        {
          "background": "transparent",
          "background_templates": [
            "{{ if gt .Code 0 }}#cc2222{{ end }}"
          ],
          "invert_powerline": true,
          "properties": {
            "always_enabled": true
          },
          "style": "powerline",
          "template": " {{ if gt .Code 0 }}{{ reason .Code }}{{ else }}✅{{ end }} ",
          "type": "status"
        },
        {
          "background": "#689f63",
          "foreground": "#ffffff",
          "invert_powerline": true,
          "powerline_symbol": "",
          "properties": {
            "fetch_version": true
          },
          "style": "powerline",
          "template": " {{ if .PackageManagerIcon }}{{ .PackageManagerIcon }} {{ end }}{{ .Full }}  ",
          "type": "node"
        },
        {
          "background": "#00acd7",
          "foreground": "#111111",
          "invert_powerline": true,
          "powerline_symbol": "",
          "properties": {
            "fetch_version": true
          },
          "style": "powerline",
          "template": " {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }}  ",
          "type": "go"
        },
        {
          "background": "#4063D8",
          "foreground": "#111111",
          "invert_powerline": true,
          "powerline_symbol": "",
          "properties": {
            "fetch_version": true
          },
          "style": "powerline",
          "template": " {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }}  ",
          "type": "julia"
        },
        {
          "background": "#AE1401",
          "foreground": "#ffffff",
          "invert_powerline": true,
          "powerline_symbol": "",
          "properties": {
            "display_mode": "files",
            "fetch_version": true
          },
          "style": "powerline",
          "template": " {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }}  ",
          "type": "ruby"
        },
        {
          "background": "#FEAC19",
          "foreground": "#ffffff",
          "invert_powerline": true,
          "powerline_symbol": "",
          "properties": {
            "display_mode": "files",
            "fetch_version": false
          },
          "style": "powerline",
          "template": " {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} ",
          "type": "azfunc"
        },
        {
          "background_templates": [
            "{{if contains \"default\" .Profile}}#FFA400{{end}}",
            "{{if contains \"jan\" .Profile}}#f1184c{{end}}"
          ],
          "foreground": "#ffffff",
          "invert_powerline": true,
          "powerline_symbol": "",
          "properties": {
            "display_default": false
          },
          "style": "powerline",
          "template": " {{ .Profile }}{{ if .Region }}@{{ .Region }}{{ end }}  ",
          "type": "aws"
        },
        {
          "background": "#0D71CE",
          "foreground": "#111111",
          "invert_powerline": true,
          "powerline_symbol": "",
          "trailing_diamond": "<#0D71CE,#0D71CE>",
          "properties": {
            "display_mode": "environment",
            "fetch_virtual_env": true,
            "home_enabled": true
          },
          "style": "powerline",
          "template": " {{ if .Error }}{{ .Error }}{{ else }}{{ .Venv }}{{ end }}  ",
          "type": "python"
        },
        {
          "type": "az",
          "style": "powerline",
          "foreground": "#d3d7cf",
          "background": "#0D71CE",
          "template": "  {{ .Name }}"
        },
        {
          "type": "session",
          "style": "diamond",
          "foreground": "#d3d7cf",
          "background": "#0D71CE",
          "trailing_diamond": "<#0D71CE,transparent>",
          "template": "{{ if .SSHSession }}  {{ end }}{{ .HostName }} "
        },
        {
          "background": "transparent",
          "foreground": "#d3d7cf",
          "style": "powerline",
          "template": "─╮",
          "type": "text"
        }
      ]
    },
    {
      "alignment": "left",
      "newline": true,
      "type": "prompt",
      "segments": [
        {
          "background": "transparent",
          "foreground": "#d3d7cf",
          "style": "powerline",
          "template": "╰─",
          "type": "text"
        }
      ]
    },
    {
      "type": "rprompt",
      "segments": [
        {
          "foreground": "#d3d7cf",
          "background": "transparent",
          "style": "powerline",
          "template": "─╯",
          "type": "text"
        }
      ]
    }
  ],
  "console_title_template": "{{ .Shell }} in {{ .Folder }}",
  "secondary_prompt": {
    "background": "transparent",
    "foreground": "#ffffff",
    "template": "-> "
  },
  "transient_prompt": {
    "background": "transparent",
    "foreground": "#FC0108",
    "template": "> "
  },
  "final_space": true,
  "version": 2
}
